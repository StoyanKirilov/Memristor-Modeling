# A complex Logical function
# containing the basic Logical operators and functions
# realized in GNU Octave
# for student and educational purposes
# operator AND
clear
clc
pkg load signal
# defining signals
# sampling
N=1e5; # number of samples
tmin=0; # start simulation
tmax=3; # stop simulation
deltat=(tmax-tmin)/N; # time step
t=tmin:deltat:tmax; # time variable
x1=(square(2*pi*2*t)+1)/2; # input signal 1
x2=(square(2*pi*2*t+deg2rad(-60))+1)/2; # input signal 2
x3=(square(2*pi*2*t+deg2rad(60))+1)/2;
#
y1=not(x1);
y2=and(x1,y1);
y3=or(x1,x2);
y4=xor(x3,y3);
y=y4;
figure(1)
subplot(4,1,1)
plot(t,x1)
axis([-0.1 3.1 -0.1 1.1])
xlabel('Time, s')
ylabel('Input')
title('Input signal')
grid
subplot(4,1,2)
plot(t,x2)
axis([-0.1 3.1 -0.1 1.1])
xlabel('Time, s')
ylabel('Input')
title('Input signal')
grid
subplot(4,1,3)
plot(t,x3)
axis([-0.1 3.1 -0.1 1.1])
xlabel('Time, s')
ylabel('Output')
title('Output signal')
grid
subplot(4,1,4)
plot(t,y)
axis([-0.1 3.1 -0.1 1.1])
xlabel('Time, s')
ylabel('Output')
title('Output signal')
grid
